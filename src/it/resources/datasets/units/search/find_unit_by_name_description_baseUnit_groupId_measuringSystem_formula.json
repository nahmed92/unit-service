{
    "_embedded": {
        "units": [
            {
                "name": "${name}",
                "groupId": "${groupId}",
                "isBaseUnit": "@contains(${baseUnit})@",
                "description": "${description}",
                "measuringSystem": "${metricSystem}",
                "formula": "${formula}",
                "_links": {
                    "self": {
                        "href": "@contains(/units)@"
                    },
                    "unit": {
                        "href": "@contains(/units)@"
                    }
                }
            }
        ]
    },
    "_links": {
        "self": {
            "href": "@endsWith(/units{?page,size,sort})@",
            "templated": true
        },
        "profile": {
            "href": "@endsWith(/profile/units)@"
        },
        "search": {
            "href": "@endsWith(/units/search)@"
        }
    },
    "page": {
        "size": 20,
        "totalElements": 1,
        "totalPages": 1,
        "number": 0
    }
}
